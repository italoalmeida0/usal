{
  "name": "usal",
  "version": "1.0.0",
  "description": "Ultimate Scroll Animation Library - 8.4KB of pure JavaScript magic - Works with React, Vue, Svelte, Solid, Vanilla JS and more",
  "main": "dist/usal.cjs.js",
  "module": "dist/usal.esm.js",
  "browser": "dist/usal.umd.js",
  "unpkg": "dist/usal.umd.min.js",
  "jsdelivr": "dist/usal.umd.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/usal.esm.js",
      "require": "./dist/usal.cjs.js",
      "browser": "./dist/usal.umd.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.esm.js",
      "require": "./dist/react.cjs.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.esm.js",
      "require": "./dist/vue.cjs.js"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.esm.js"
    },
    "./solid": {
      "types": "./dist/solid.d.ts",
      "import": "./dist/solid.esm.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "node build.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "animation", "javascript", "react", "vue", "svelte", "solid", "vite",
    "webpack", "rollup", "parcel", "universal", "framework-agnostic"
  ],
  "author": "Italo Almeida",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "vue": ">=3.0.0",
    "solid-js": ">=1.0.0",
    "svelte": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": { "optional": true },
    "vue": { "optional": true },
    "solid-js": { "optional": true },
    "svelte": { "optional": true }
  }
}